{
  "name": "nestjs-serverless-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "Walid KARRAY",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "watch": "nest build -w",
    "sls": "dotenv -e .env.${STAGE:=dev} -- sls",
    "typeorm": "dotenv -e .env.${STAGE:=local} -- node --require ts-node/register ./node_modules/typeorm/cli.js -f ./typeorm-cli.db.config.ts",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "dotenv -e .env.local -- nest start",
    "start:dev": "dotenv -e .env.local -- nest start --watch",
    "start:debug": "dotenv -e .env.${STAGE:=dev} --nest start --debug --watch",





    
    "start:prod": "dotenv -e .env.${STAGE:=dev} -- node dist/main",
    "dcup": "dotenv -e .env.local -- docker-compose up",
    "dcupd": "dotenv -e .env.local -- docker-compose up -d",
    "dcd": "dotenv -e .env.local -- docker-compose down",
    "sls:offline": "STAGE=local yarn sls offline cloudside --stage dev",
    "sls:deploy": "yarn sls deploy --stage ${STAGE:=dev}",
    "sls:deploy:table": "yarn sls deploy --config serverless-dynamodb-table.yaml --stage ${STAGE:=dev}",
    "sls:remove": "yarn sls remove --stage ${STAGE:=dev}",
    "sls:remove:table": "yarn sls remove --config serverless-dynamodb-table.yaml --stage ${STAGE:=dev}",
    "sls:logs:nest": "yarn sls logs -f nest -t -s ${STAGE:=dev}",
    "sls:logs:sqs": "yarn sls logs -f sqs -t -s ${STAGE:=dev}",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "schema:drop": "yarn typeorm schema:drop",
    "schema:sync": "yarn typeorm schema:sync"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.76.0",
    "@aws-sdk/client-ses": "^3.72.0",
    "@aws-sdk/client-sqs": "^3.72.0",
    "@nestjs-plus/discovery": "^2.0.2",
    "@nestjs/apollo": "^10.0.8",
    "@nestjs/axios": "^0.0.7",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/event-emitter": "^1.1.0",
    "@nestjs/graphql": "^10.0.8",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/terminus": "^8.0.6",
    "@nestjs/typeorm": "^8.0.3",
    "@vendia/serverless-express": "^4.5.4",
    "apollo-server-express": "^3.6.7",
    "aws-sdk": "^2.1116.0",
    "class-transformer": "0.4.0",
    "class-validator": "^0.13.2",
    "graphql": "^16.3.0",
    "jwks-rsa": "^2.0.5",
    "mustache": "^4.2.0",
    "nodemailer": "^6.7.3",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.2.37",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/aws-sdk": "^2.7.0",
    "@types/aws-serverless-express": "^3.3.5",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/mustache": "^4.1.2",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.4",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.2.5",
    "lint-staged": "^12.4.0",
    "prettier": "^2.3.2",
    "serverless": "^3.14.0",
    "serverless-cloudside-plugin": "^1.0.6",
    "serverless-deployment-bucket": "^1.5.2",
    "serverless-offline": "^8.7.0",
    "serverless-offline-sqs": "^6.0.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.{json,yml,yaml,graphql}": [
      "prettier --write"
    ],
    "*.{js,ts}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
